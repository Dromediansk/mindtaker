{
  "expo": {
    "name": "mindtaker",
    "slug": "mindtaker",
    "scheme": "mindtaker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dromediansk.mindtaker"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dromediansk.mindtaker"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.243209097004-43t6pqdao3m7iqo9netcuc1mgbv2h641"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/WorkSans-Bold.ttf",
            "./assets/fonts/WorkSans-Light.ttf",
            "./assets/fonts/WorkSans-Regular.ttf",
            "./assets/fonts/WorkSans-SemiBold.ttf"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "c3a76208-447f-4c87-94b5-b3c6447a34f1"
      }
    },
    "owner": "dromediansk"
  }
}
